{
  "manifest_version": 3,
  "name": "Tab Title Extension",
  "version": "1.0",
  "description": "Displays the title of the current tab in the popup.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/logo_16.png",
    "32": "/images/logo_32.png",
    "48": "/images/logo_48.png"
  }
}
